{
  "name": "hostelpulse-clean",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:demo": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:e2e": "playwright test",
    "test:all": "pnpm test && pnpm test:e2e",
    "postinstall": "prisma generate",
    "spec": "pnpm .venv-speckitty/bin/spec-kitty",
    "spec:specify": "$(cd $(git rev-parse --git-common-dir)/.. && pwd)/.kittify/scripts/bash/create-new-feature.sh --mission=hostelpulse-mvp",
    "spec:plan": "$(cd $(git rev-parse --git-common-dir)/.. && pwd)/.kittify/scripts/bash/setup-plan.sh",
    "spec:tasks": "$(cd $(git rev-parse --git-common-dir)/.. && pwd)/.kittify/scripts/bash/refresh-kittify-tasks.sh",
    "spec:accept": "$(cd $(git rev-parse --git-common-dir)/.. && pwd)/.kittify/scripts/bash/accept-feature.sh",
    "spec:merge": "$(cd $(git rev-parse --git-common-dir)/.. && pwd)/.kittify/scripts/bash/merge-feature.sh",
    "spec:setup": "python3 -m venv .venv-speckitty && .venv-speckitty/bin/pip install spec-kitty-cli"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "5.22.0",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-auth": "5.0.0-beta.30",
    "papaparse": "^5.5.3",
    "prisma": "^5.22.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.68.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.26",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.22",
    "daisyui": "^5.5.13",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.1.6",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
